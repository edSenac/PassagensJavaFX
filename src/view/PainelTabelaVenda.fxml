<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="painelTabelaVenda" prefHeight="350.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.VendaController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" onAction="#tratarBotaoCadastrar" text="Cadastrar" />
          <Button mnemonicParsing="false" onAction="#tratarBotaoEditar" text="Editar" />
          <Button mnemonicParsing="false" onAction="#tratarBotaoRemover" text="Remover" />
        </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ToolBar>
      <TableView fx:id="tableViewVendas" prefHeight="313.0" prefWidth="493.0">
        <columns>
          <TableColumn fx:id="tableColumnCliente" prefWidth="150.0" text="Cliente" />
          <TableColumn fx:id="tableColumnVoo" prefWidth="150.0" text="Voo" />
          <TableColumn fx:id="tableColumnHorario" prefWidth="120.0" text="Horario" />
        </columns>
      </TableView>
   </children>
</VBox>
