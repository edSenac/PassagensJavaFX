<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="painelTabelaVoo" prefHeight="350.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.VooController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" onAction="#tratarBotaoCadastrar" text="Cadastrar" />
            <Button mnemonicParsing="false" onAction="#tratarBotaoEditar" text="Editar" />
            <Button mnemonicParsing="false" onAction="#tratarBotaoRemover" text="Remover" />
        </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ToolBar>
      <TableView fx:id="tableViewVoos" prefHeight="313.0" prefWidth="493.0">
        <columns>
          <TableColumn fx:id="tableColumnOrigem" prefWidth="150.0" text="Origem" />
          <TableColumn fx:id="tableColumnDestino" prefWidth="150.0" text="Destino" />
            <TableColumn fx:id="tableColumnAviao" prefWidth="100.0" text="Aviao" />
            <TableColumn fx:id="tableColumnHorario" prefWidth="120.0" text="Horario" />
            <TableColumn fx:id="tableColumnLugares" prefWidth="138.0" text="Lugares" />
        </columns>
      </TableView>
   </children>
</VBox>
