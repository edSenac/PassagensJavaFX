<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="painelTabelaAviao" prefHeight="350.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AviaoController">
    <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" onAction="#tratarBotaoCadastrar" text="Cadastrar" />
            <Button mnemonicParsing="false" onAction="#tratarBotaoEditar" text="Editar" />
            <Button mnemonicParsing="false" onAction="#tratarBotaoRemover" text="Remover" />
        </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ToolBar>
      <TableView fx:id="tableViewAvioes" prefHeight="314.0" prefWidth="420.0">
        <columns>
          <TableColumn fx:id="tableColumnNome" prefWidth="204.0" text="Nome do Avião" />
            <TableColumn fx:id="tableColumnAssentos" prefWidth="215.0" text="Número de Assentos" />
        </columns>
      </TableView>
    </children>
</VBox>
